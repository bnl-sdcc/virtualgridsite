#!/usr/bin/env python 

import classad
import sys

import virtualgridsite.hooks


stdin = sys.stdin.readlines()
source_ad = []
completed_ad = []
ad = source_ad
for line in stdin:
    if line.startswith('------'):
        ad = completed_ad
    else:
        ad.append(line)
        

newad = virtualgridsite.hooks.exit()
if newad:
    print newad.printOld()
sys.exit(0)

